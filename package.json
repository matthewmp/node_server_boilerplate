{
  "name": "webpack_node_mongo_boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate: Node, Express, Mongoose, Webpack",
  "main": "main.js",
  "scripts": {
    "buildClient": "webpack --config=webpack.config.js",
    "buildServer": "babel ./src/server -d ./dist",
    "start:server": "nodemon ./src/server/server.js",
    "start:dev": "webpack-dev-server",
    "launch": "npm run buildClient && npm run buildServer && npm run start:server"
  },
  "author": "Matt Palumbo",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "extract-loader": "^1.0.2",
    "file-loader": "^1.1.9",
    "html-loader": "^0.5.5",
    "nodemon": "^1.15.1",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "mongoose": "^5.0.7",
    "morgan": "^1.9.0"
  }
}
